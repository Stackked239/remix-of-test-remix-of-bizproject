{
  "name": "bizhealth-report-pipeline",
  "version": "1.0.0",
  "description": "BizHealth comprehensive business report generation pipeline",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc --noEmit false || true",
    "start": "tsx src/render-worker.ts",
    "worker": "tsx src/render-worker.ts",
    "pipeline": "tsx src/run-pipeline.ts",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:transform": "tsx tests/test-transformation.ts",
    "test:phase1": "tsx tests/test-phase1.ts",
    "test:phase2": "tsx tests/test-phase2.ts",
    "test:phase3": "tsx tests/test-phase3.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "validate:reports": "tsx src/orchestration/reports/validation/validate-reports.ts",
    "validate:ascii": "tsx scripts/ci-validate-reports.ts",
    "generate:validated": "npm run start && npm run validate:reports",
    "pipeline:validate": "npm run start && npm run validate:ascii",
    "test:reports": "npm run validate:reports",
    "test:ascii": "npx vitest run test/visualization/ascii-validation.test.ts",
    "test:mappings": "npx vitest run src/orchestration/reports/config/__tests__/section-mapping.test.ts",
    "validate:all": "npm run test:mappings && npm run validate:reports && npm run validate:ascii",
    "ci": "npm run pipeline:validate && npm test",
    "test:formatting": "vitest run src/qa/__tests__/formatting-equivalency.test.ts",
    "test:css": "vitest run src/qa/__tests__/css-usage.test.ts",
    "test:formatting:watch": "vitest watch src/qa/__tests__/",
    "test:formatting:update-snapshots": "vitest run --update src/qa/__tests__/",
    "generate:test-reports": "tsx src/qa/scripts/generate-test-reports.ts",
    "generate:qa-samples": "tsx src/qa/scripts/generate-qa-samples.ts",
    "qa:full": "npm run generate:test-reports && npm run test:formatting && npm run test:css",
    "test:visual": "vitest run src/__tests__/phase5-visual-validation.test.ts --reporter=verbose",
    "report:audit": "npm run start && npm run test:visual",
    "phase5:validate": "npm run test:visual"
  },
  "keywords": [
    "business-analysis",
    "ai",
    "batch-api",
    "anthropic",
    "claude"
  ],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@supabase/supabase-js": "^2.39.0",
    "canvas": "^2.11.2",
    "chart.js": "^3.9.1",
    "chartjs-node-canvas": "^4.1.6",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^10.3.10",
    "marked": "^17.0.1",
    "pg": "^8.11.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsdom": "^21.1.6",
    "@types/marked": "^5.0.2",
    "@types/node": "^20.10.5",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "jsdom": "^24.0.0"
  }
}
